
public class Edge {

	public Node a, b;
	public int color;
	public String classname;
	
	public Edge(Node a, Node b, int color)
	{
		this.a = a; 
		this.b = b;
		this.color = color;
	}
	
	
	public Edge(Node a, Node b) 
	{
		this(a, b, 0);
		
	}
	
	public boolean contains(Node n)
	{
		return a.equals(n) || b.equals(n);
	}
	
}
